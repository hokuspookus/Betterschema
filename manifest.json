{
  "manifest_version": 3,
  "name": "Betterschema",
  "version": "1.5.0",
  "description": "GÃ¶r det Skola24 inte kunde ;)",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "scripts/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js", 
        "scripts/notes.js", 
        "scripts/dynamicTime.js", 
        "scripts/matsedel.js", 
        "scripts/dark_mode.js",
        "scripts/user_count.js"
      ],

      "css": ["scripts/notes.css"],
      "matches": [
        "https://web.skola24.se/timetable/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
      "48": "images/bs_48.png"
  }
}